{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "created at",
    "id",
    "kind",
    "name",
    "plan",
    "status",
    "updated_at"

   ],

   "properties": {
     "created at": {
       "$id": "#/properties/created at",
       "type": "string",
       "title": "The Created at Schema",
       "default": "",
       "examples": [
         "2019-01-07T21:42:45Z"
       ],
       "pattern": "^(.*)$"
     },

     "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "account"
      ],
      "pattern": "^(.*)$"
    },


     "plan": {
      "$id": "#/properties/plan",
      "type": "string",
      "title": "The Plan Schema",
      "default": "",
      "examples": [
        "Free"
      ],
      "pattern": "^(.*)$"
    },




     "updated at": {
       "$id": "#/properties/updated at",
       "type": "string",
       "title": "The Updated at Schema",
       "default": "",
       "examples": [
         "2019-01-07T21:42:45Z"
       ],
       "pattern": "^(.*)$"
     },
     "name": {
       "$id": "#/properties/name",
       "type": "string",
       "title": "The Name Schema",
       "default": "",
       "examples": [
         "Galactic Empire"
       ],
       "pattern": "^(.*)$"
     },
     "id": {
       "$id": "#/properties/id",
       "type": "integer",
       "title": "The id Schema",
       "default": 0,
       "examples": [
         100
       ],
       "pattern": "^(.*)$"
     },
     "status": {
       "$id": "#/properties/status",
       "type": "string",
       "title": "The Status Schema",
       "default": "",
       "examples": [
         "active"
       ]
     }
   }
}