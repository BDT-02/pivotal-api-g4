{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "kind",
    "id",
    "name",
    "person_id",
    "project_ids"
  ],
  "properties": {
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "workspace"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        722862
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "A new workspace"
      ],
      "pattern": "^(.*)$"
    },
    "person_id": {
      "$id": "#/properties/person_id",
      "type": "integer",
      "title": "The Person_id Schema",
      "default": 0,
      "examples": [
        3128921
      ]
    },
    "project_ids": {
      "$id": "#/properties/project_ids",
      "type": "array",
      "title": "The Project_ids Schema"
    }
  }
}